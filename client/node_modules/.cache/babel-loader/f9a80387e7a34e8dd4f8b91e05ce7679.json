].style.padding = gap + \"px\";\n        }\n\n        for (i = 1; i < n; i++) {\n          images[i].style.transformOrigin = \"50% 50% \" + -apothem + \"px\";\n          images[i].style.transform = \"rotateY(\" + i * theta + \"rad)\";\n        }\n\n        if (bfc) for (i = 0; i < n; i++) {\n          images[i].style.backfaceVisibility = \"hidden\";\n        }\n        rotateCarousel(currImage);\n      }\n\n      function setupNavigation() {\n        nav.addEventListener(\"click\", onClick, true);\n\n        function onClick(e) {\n          e.stopPropagation();\n          var t = e.target;\n          if (t.tagName.toUpperCase() != \"IMG\") return;\n\n          if (t.classList.contains(\"next\")) {\n            currImage++;\n          } else {\n            currImage--;\n          }\n\n          rotateCarousel(currImage);\n        }\n      }\n\n      function rotateCarousel(imageIndex) {\n        figure.style.transform = \"rotateY(\" + imageIndex * -theta + \"rad)\";\n      }\n    }\n  }\n};",{"version":3,"sources":["VIPCarouselDDD.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+WA;AACA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA,EAAA;AACA,GAHA;AAIA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,EAAA,kBAAA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AAHA,GAJA;AASA,EAAA,OATA,qBASA;AACA,QAAA,SAAA,GAAA,QAAA,CAAA,aAAA,CAAA,WAAA,CAAA;AACA,IAAA,QAAA,CAAA,SAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,EAAA,CAAA;;AAEA,aAAA,QAAA,CAAA,IAAA,EAAA,CAAA,EAAA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,aAAA,CAAA,QAAA,CAAA;AAAA,UACA,GAAA,GAAA,IAAA,CAAA,aAAA,CAAA,KAAA,CADA;AAAA,UAEA,MAAA,GAAA,MAAA,CAAA,QAFA;AAAA,UAGA,CAAA,GAAA,MAAA,CAAA,MAHA;AAAA,UAIA,GAAA,GAAA,IAAA,CAAA,OAAA,CAAA,GAAA,IAAA