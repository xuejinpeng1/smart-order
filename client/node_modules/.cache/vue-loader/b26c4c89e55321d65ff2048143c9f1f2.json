ction clock(){\r\n            num>0 ? num-- : clearInterval(int);\r\n            document.getElementById(\"time\").innerHTML=num;\r\n             console.log(num)\r\n        }\r\n    },\r\n    /* 获取验证码 */\r\n    yanzheng(){\r\n        var yanzheng = document.getElementById(\"yanzheng\");\r\n        var yanzheng2 = document.getElementById(\"yanzheng2\");\r\n        \r\n        yanzheng.onclick = function () {\r\n           yanzheng.style.display = \"none\";\r\n           yanzheng2.style.display = \"block\";           \r\n        }\r\n    },    \r\n  },\r\n   mounted() {\r\n    this.yanzheng(),\r\n    this.times()\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n     .setPassword{\r\n        background:#eee;   \r\n        // 修改密码\r\n    .mint-header{\r\n              padding-top:0.4rem;\r\n              padding-bottom:0.2rem;\r\n              background:#fff;\r\n              color:#000;\r\n              font-size:0.35rem;       \r\n          } \r\n          input{\r\n              width:94%;\r\n              padding:0.2rem;\r\n              border:none;\r\n              font-size:0.3rem;\r\n          }     \r\n          .input{\r\n              width:100%;height:0.7rem;\r\n              background:#fff;\r\n              display: flex;\r\n              justify-content: space-between;\r\n              margin-top:0.2rem;\r\n          }\r\n          .input_yz{\r\n              width:4rem;\r\n          }\r\n          .yanzheng{\r\n              width:1.5rem;height:100%;\r\n              font-size:0.1rem;\r\n              color:#e00;\r\n              line-height:0.7rem;\r\n              margin-right:0.2rem;  \r\n          }\r\n          .yanzheng2{\r\n              color:#ccc;\r\n          }\r\n          .button_ok{\r\n              width:97%;\r\n              padding:0.15rem;\r\n              border:none;\r\n              border-radius:0.1rem;\r\n              background:#ccc;\r\n              color:#fff;\r\n              font-size:0.3rem;\r\n          }\r\n     